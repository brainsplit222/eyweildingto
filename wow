local methods = {
    English = EnglishMapping,
    Language = LanguageMapping,
    Unlegit = UnlegitMapping,
    Circle = CircleMapping,
    Diacritics = DiacriticsMapping,
    Underline = UnderlineMapping,
    Arrow = ArrowMapping
