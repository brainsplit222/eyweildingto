                        Rayfield:Notify({
                            Title = "Legacy Chat",
                            Content = "Auto Bypass does not work on legacy chat, try another game.",
                            Duration = 6.5,
                            Image = 4483362458
                        })
                    end
                end
            else
                if autoBool then
                    Rayfield:Notify({
                        Title = "Legacy Chat",
                        Content = "Auto Bypass does not work on legacy chat, try another game.",
                        Duration = 6.5,
                        Image = 4483362458
                    })
                end
            end
        end
    end
})

